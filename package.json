{
  "name": "gameboyish",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "http-server -o -c-1 -p 9000 public"
  },
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "build": {
    "backend": "webpack",
    "extensions": [
      "typescript"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.1",
    "http-server": "^14.1.0",
    "rollup": "^2.70.1"
  }
}
